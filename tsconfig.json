{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "lib": ["es6", "dom"],
    "module": "none",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    // "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "outDir": "dist",
    "pretty": true,
    "sourceMap": true,
    // "strictNullChecks": true,
    "target": "es6"
  },
  "include": ["**/*"]
}
